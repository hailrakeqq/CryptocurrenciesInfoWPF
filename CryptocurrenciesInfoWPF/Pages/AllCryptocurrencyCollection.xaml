<Page x:Class="CryptocurrenciesInfoWPF.Pages.AllCryptocurrencyCollection"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:CryptocurrenciesInfoWPF.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="800" d:DesignWidth="800"
      Title="AllCryptocurrencyCollection"
      Style="{DynamicResource PageStyle}">

    <Grid  Height="800">
        <ListBox x:Name="cryptocurrencyCollection"  Width="355" Height="300" Style="{DynamicResource ListBoxStyle}" Margin="225,250,225,250" SelectionChanged="GetIDAndRedirectToCryptocurrencyPage"/>
        <Button Click="GoBackToMainMenu" Content="Go Back" Width="150" Height="50" Margin="325,74,325,676" Style="{DynamicResource ButtonStyle}"/>
    </Grid>
</Page>
